{% extends 'baseFront.html.twig' %}

{% block title %}Accueil!{% endblock %}

{% block body %}
<div class="container">
    <main>
        <div class="py-5 text-center">
            <h2>Reservation</h2>
            <p class="lead">Veuillez vous inscrire afin de bénéficier des avantages des cours en ligne</p>
        </div>
        {% for message in app.flashes('alertCreate') %}
        <div class="alert alert-warning alert-dismissible fade show">
            <strong> <i class="fa fa-circle-exclamation"></i> Info! </strong>
            {{message}}
            <button type="button" class="btn-close" data-bs-dismiss="alert"></span>
        </div>
        {% endfor%}
        <div class="row g-5">
            <div class="col-md-1 col-lg-2">

            </div>
            <div class="col-md-7 col-lg-8">
                {{ include('etudiant/_form.html.twig') }}
            </div>
        </div>
    </main>
</div>
{% endblock %}